{
  "name": "starter-expo-template",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "start:local": "npx expo start",
    "start:cache": "npx expo start --clear",
    "start:fast": "EXPO_USE_EXOTIC=1 expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "lint": "eslint src && yarn tsc",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "fix": "npx expo install --check",
    "format": "prettier --write .",
    "publish": "eas update",
    "login": "eas login",
    "logout": "eas logout",
    "register": "eas register",
    "whoami": "eas whoami",
    "build:ios": "eas build:ios",
    "build:android": "eas build:android",
    "build": "yarn build:ios && yarn build:android",
    "prebuild": "npx expo prebuild",
    "build:preview": "eas build -p android --profile preview",
    "upload:ios": "expo upload:ios",
    "upload:android": "expo upload:android",
    "upload": "yarn upload:ios && yarn upload:android"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@reduxjs/toolkit": "^2.2.5",
    "expo": "~51.0.8",
    "expo-constants": "^16.0.1",
    "expo-device": "^6.0.2",
    "expo-linking": "^6.3.1",
    "expo-notifications": "^0.28.3",
    "expo-router": "^3.5.14",
    "expo-splash-screen": "^0.27.4",
    "expo-status-bar": "^1.12.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-reanimated": "^3.11.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.3.0",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/lodash": "^4.17.4",
    "@types/react": "~18.2.45",
    "prettier": "^3.2.5",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
